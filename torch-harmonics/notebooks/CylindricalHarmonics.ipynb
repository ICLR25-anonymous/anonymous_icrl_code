   "id": "db9edb9e-7046-46e0-8114-f00a92c8972b",
   "metadata": {},
   "source": [
    "# Cylindrical Harmonics Transform"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "3995623e-e4b9-4ecd-8e28-33a1545ee547",
   "metadata": {},
   "outputs": [],
   "source": [
    "import torch\n",
    "from torch import nn"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "7694a933-bfea-40d6-a578-d4f25681bbd9",
   "metadata": {},
   "outputs": [],
   "source": [
    "def f(r, phi, z):\n",
    "    return r**2 + np.cos(2*phi) - z**2 + r*np.sin(phi)*z\n",
    "\n",
    "r = torch.linspace(0,1,10)\n",
    "phi = torch.linspace(0,2*np.pi,36)\n",
    "z = torch.linspace(0,1,10)\n",
    "y = f(r[:,None,None], phi[None,:,None], z[None,None,:])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "0b320c69-762e-4e3a-9b26-12c5bdfb4010",
   "metadata": {},
   "outputs": [],
   "source": [
    "plotting.plot_cylinder_fn(y.numpy(), fig=plt.figure(figsize=(2,2)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "67154549-5bc5-4c7d-86fa-66bca063fb8d",
   "metadata": {},
   "outputs": [],
   "source": [
    "class CHT(nn.Module):\n",
    "    def __init__(self, K=1, L=3, M=1, num_radii=10, num_phi=36, num_height=10):\n",
    "        super().__init__()\n",
    "        self.w_lin = nn.Linear(num_radii * num_phi * num_height, M*K*(L*2+1))\n",
    "        self.ch = CylindricalHarmonics(K,L,M, num_radii=num_radii, num_phi=num_phi, num_height=num_height)\n",
    "\n",
    "    def forward(self, x):\n",
    "        w = self.w_lin(x)\n",
    "        return self.ch(w), w.detach()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "2fe65c6a-db88-4228-a2f0-f30ad22f1269",
   "metadata": {},
   "outputs": [],
   "source": [
    "K = 2\n",
    "L = 3\n",
    "M = 2\n",
    "ch_model = CHT(K,L,M)\n",
    "optimizer = torch.optim.Adam(ch_model.parameters(), lr = 1e-4)\n",
    "\n",
    "for iter in range(200):\n",
    "    y_pred, _ = ch_model(y.view(1,-1))\n",
    "    loss = (y_pred - y).pow(2).mean()\n",
    "\n",
    "    optimizer.zero_grad()\n",
    "    loss.backward()\n",
    "    optimizer.step()\n",
    "\n",
    "    if iter % 10 == 0:\n",
    "        print(f'iteration: {iter} loss: {loss.item()}')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "12467133-f13b-4982-a472-9f21c2cd8835",
   "metadata": {},
   "outputs": [],
   "source": [
    "with torch.no_grad():\n",
    "    pred, w = ch_model(y.view(1,-1))\n",
    "    plotting.plot_cylinder_fn(pred.squeeze().numpy(), fig=plt.figure(figsize=(2,2)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "1d4eb9d7-0e46-4e7e-857c-c642b9aebe2e",
   "metadata": {},
   "outputs": [],
   "source": [
    "w_ph = torch.einsum(\"bwrpz,bw->bwrpz\", ch_model.ch.Psi, w).view(M,K,2*L+1, ch_model.ch.num_radii, ch_model.ch.num_phi, ch_model.ch.num_height)\n",
    "w.numpy().round(2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "6ce77434-8b0d-4b15-a423-1298a7b6b4f3",
   "metadata": {},
   "outputs": [],
   "source": [
    "fig = plt.figure(figsize=(4*L, K*M))\n",
    "subfigs = fig.subfigures(K*M, L*2+1)\n",
    "for m in range(M):\n",
    "    for k in range(K):\n",
    "        for l in range(L*2+1):\n",
    "            plotting.plot_cylinder_fn(w_ph[m,k,l].numpy(), fig=subfigs[m*k+K, l], vmin=w_ph.min(), vmax=w_ph.max())"
   ]
  },
